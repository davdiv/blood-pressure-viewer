{
  "name": "blood-pressure-viewer",
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "ci": "npm run build && npm run check",
    "dev": "vite",
    "build": "npm run build:logo && npm run build:app",
    "build:logo": "rm -rf public && svgo src/logo.svg -o public/logo.svg && pwa-assets-generator --preset minimal-2023 public/logo.svg",
    "build:app": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsc -p tsconfig.node.json"
  },
  "devDependencies": {
    "@amadeus-it-group/tansu": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@tsconfig/svelte": "^5.0.4",
    "@types/node": "^24.2.0",
    "@types/web-bluetooth": "^0.0.21",
    "@vite-pwa/assets-generator": "^1.0.0",
    "bootstrap": "^5.3.7",
    "chart.js": "^4.5.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-plugin-zoom": "^2.2.0",
    "date-fns": "^4.1.0",
    "svelte": "^5.35.5",
    "svelte-check": "^4.2.2",
    "svgo": "^4.0.0",
    "typescript": "^5.9.2",
    "vite": "^7.0.4",
    "vite-plugin-pwa": "^1.0.2"
  }
}
